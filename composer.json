{
    "name": "pixelopen/kirby-news",
    "type": "kirby-plugin",
    "description": "A plugin for Kirby 3 CMS to add news module",
    "homepage": "https://github.com/Pixel-Developpement/kirby-dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Pixel Open",
            "homepage": "https://pixel-open.org/"
        }
    ],
    "keywords": [
        "kirby3",
        "kirby3-cms",
        "kirby3-plugin"
    ],
    "require": {
        "getkirby/composer-installer": "^1.1",
        "getkirby/cms": "^3.6.0||^4.0.0",
        "fabianmichael/kirby-meta": "^0.4.2-beta"
    },
    "config": {
        "optimize-autoloader": true,
        "allow-plugins": {
            "getkirby/composer-installer": true
        }
    },
    "require-dev": {
        "symplify/easy-coding-standard": "^12.0"
    }
}
